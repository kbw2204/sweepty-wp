extends layout

block content
	.carouselSlide(data-ride="carousel")
		.carousel-inner
			.carousel-item.active
				img.d-block(src="images/team.jpg",alt="First slide")
			.carousel-item
				img.d-block(src="images/music.jpg", alt="Second slide")
			.carousel-item
				img.d-block(src="images/sports.jpg", alt="Third slide")

	#white-box.l-align-center.homepage-search-card.card.card--elevated-3
		#search-group.container
			.row
				.col-12
					h1.hdie-small-accessible
						| 모두의 모임
				#search-form.col-lg-offset-3.col-lg-9
					.input-group
						input.form-control(type="text", placeholder="Search events or categories", aria-label="search-events")
						input#region-selector.form-control(type="text", placeholder="City or location", aria-label="select region", value="Seoul,Republic of Korea")
						select#date-selecter.form-control
							option All Dates
							option Today
							option Tommorrow
							option This Week
							option Next Week
							option Next Month
						span#search1.input-group-btn
							button.btn.btn-search(type="button")
								i.fa.fa-search(aria-hidden="true")
						span#search2.input-group-btn
							button.btn.btn-searcg(type="button") SEARCH
